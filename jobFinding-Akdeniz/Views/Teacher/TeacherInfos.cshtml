                    <div class="educationInfo cd-form" id="languageInfo">
                        <h4 id="list-item-1">Yabancı Dil</h4>
                        <div class="panel-group">
                            @foreach (var language in Model.user_language_skill.ToList())
                            {
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h4 class="panel-title">
                                            <a data-toggle="collapse" href="#language_@language.skillId"><i class="fas fa-check-circle"></i> @language.languages.languageName</a>
                                        </h4>
                                    </div>
                                    <div id="language_@language.skillId" class="panel-collapse collapse">
                                        <div class="panel-body">
                                            <div class="text-danger small">
                                                <div class="col-lg-12">
                                                    @ViewBag.Warning
                                                </div>
                                            </div>
                                            <div class="text-success small">
                                                <div class="col-lg-12">
                                                    @ViewBag.Success
                                                </div>
                                            </div>
                                            @using (Html.BeginForm("EditLanguage", "Teacher", FormMethod.Post))
                                            {
                                                @Html.AntiForgeryToken()
                                                <input type="hidden" value="@language.skillId" name="skillId" />
                                                <div class="form-group cd-form">
                                                    <div class="col-lg-6">
                                                        <label><h5>Dil</h5></label>
                                                        <select class="form-control" name="languageID" required>
                                                            <option class="text-muted" disabled>Dil Seçiniz:</option>
                                                            @foreach (var item in languages)
                                                            {
                                                                <option value="@item.languageId" @if (item.languageId == language.languageID) { <text> selected="selected" </text> }>@item.languageName</option>
                                                            }
                                                        </select>
                                                        <div class="form-group error">
                                                            <div class="col-lg-6">

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group cd-form">
                                                    <div class="col-lg-6">
                                                        <label><h5>Seviye</h5></label>
                                                        <select class="form-control" name="level" required>
                                                            <option class="text-muted" disabled>Seviye Seçiniz:</option>
                                                            <option value="1" @if ("1" == language.level) { <text> selected="selected" </text> }>1</option>
                                                            <option value="2" @if ("2" == language.level) { <text> selected="selected" </text> }>2</option>
                                                            <option value="3" @if ("3" == language.level) { <text> selected="selected" </text> }>3</option>
                                                            <option value="4" @if ("4" == language.level) { <text> selected="selected" </text> }>4</option>
                                                            <option value="5" @if ("5" == language.level) { <text> selected="selected" </text> }>5</option>

                                                        </select>
                                                        <div class="form-group error">
                                                            <div class="col-lg-6">

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
