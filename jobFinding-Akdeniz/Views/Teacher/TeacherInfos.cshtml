                                        <a data-toggle="collapse" href="#newExperience"><i class="fas fa-plus-circle"></i> Yeni Deneyim Ekle</a>
                                    </h4>
                                </div>
                                <div id="newExperience" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="text-danger small">
                                            <div class="col-lg-12">
                                                @ViewBag.Warning
                                            </div>
                                        </div>
                                        <div class="text-success small">
                                            <div class="col-lg-12">
                                                @ViewBag.Success
                                            </div>
                                        </div>
                                        @using (Html.BeginForm("AddExperience", "Teacher", FormMethod.Post))
                                        {
                                            @Html.AntiForgeryToken()
                                            @Html.HiddenFor(x => x.userAccountId)
                                            <div class="form-group cd-form">
                                                <div class="col-lg-6">
                                                    <label><h5>Şirket Adı</h5></label>
                                                    <input type="text" name="companyName" class="form-control" required />
