                                                </div>
                                                <div class="form-group cd-form">
                                                    <div class="col-lg-6">
                                                        <label><h5>Bitiş Tarihi</h5></label>
                                                        <div class="controls" style="position: relative">
                                                            <input type="text" class="form-control" id="datetimepicker-@experience.experienceId" value="@experience.endDate" name="endDate" />
                                                        </div>
                                                        <div class="form-group error">
                                                            <div class="col-lg-6">

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group cd-form">
                                                    <div class="col-lg-8">
                                                        <label><h5>Açıklama</h5></label>
                                                        <textarea class="form-control" placeholder="Açıklama" name="description">@experience.description</textarea>
                                                        <div class="form-group error">
                                                            <div class="col-lg-6">

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="col-lg-12">
                                                        <div class="col-lg-6 col-xs-6">
                                                            <a class="open-deleteExperience btn btn-danger" data-id="@experience.experienceId"><i class="fas fa-trash"></i> Sil</a>
                                                        </div>
                                                        <div class="col-lg-6 col-xs-6">
                                                            <button class="btn btn-success" type="submit"><i class="glyphicon glyphicon-ok-sign"></i> Kaydet</button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <script type="text/javascript">
                                                    $(function () {
                                                        $('#datetimepicker_@experience.experienceId').datetimepicker({
                                                            viewMode: 'days',
                                                            format: 'DD/MM/YYYY',
                                                            locale: 'tr',
                                                            //minDate: moment()
                                                        });
                                                    });
                                                    $(function () {
                                                        $('#datetimepicker-@experience.experienceId').datetimepicker({
                                                            viewMode: 'days',
                                                            format: 'DD/MM/YYYY',
                                                            locale: 'tr',
                                                            minDate: moment()
                                                        });
                                                    });
                                                </script>

                                            }
                                        </div>
                                        <div class="panel-footer"></div>
                                    </div>
                                </div>
